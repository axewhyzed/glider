{
  "name": "quotes_js",
  "base_url": "http://quotes.toscrape.com/js/",
  "mode": "pagination",
  "use_playwright": true,
  "wait_for_selector": "div.quote",
  "fields": [
    {
      "name": "quotes",
      "selector": "div.quote",
      "is_list": true,
      "children": [
        {
          "name": "text",
          "selector": "span.text",
          "transformers": ["strip"]
        },
        {
          "name": "author",
          "selector": "small.author"
        },
        {
          "name": "tags",
          "selector": "div.tags a.tag",
          "is_list": true
        }
      ]
    }
  ],
  "pagination": {
    "selector": "li.next a",
    "max_pages": 5
  }
}
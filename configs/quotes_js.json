{
    "name": "Quotes_To_Scrape_Practice",
    "mode": "pagination",
    "base_url": "https://quotes.toscrape.com/page/1/",
    "use_playwright": false,
    "respect_robots_txt": true,
    "use_checkpointing": true,
    "concurrency": 3,
    "rate_limit": 5,
    "min_delay": 1,
    "max_delay": 2,
    "pagination": {
        "selector": {
            "type": "css",
            "value": "li.next a"
        },
        "max_pages": 10
    },
    "fields": [
        {
            "name": "quotes",
            "is_list": true,
            "selectors": [
                {
                    "type": "css",
                    "value": "div.quote"
                }
            ],
            "children": [
                {
                    "name": "text",
                    "selectors": [
                        {
                            "type": "css",
                            "value": "span.text"
                        }
                    ],
                    "transformers": [
                        {
                            "name": "strip",
                            "args": []
                        }
                    ]
                },
                {
                    "name": "author",
                    "selectors": [
                        {
                            "type": "css",
                            "value": "small.author"
                        }
                    ],
                    "transformers": [
                        {
                            "name": "strip",
                            "args": []
                        }
                    ]
                },
                {
                    "name": "author_url",
                    "selectors": [
                        {
                            "type": "xpath",
                            "value": ".//span/a/@href"
                        }
                    ],
                    "transformers": [
                        {
                            "name": "strip",
                            "args": []
                        }
                    ]
                }
            ]
        },
        {
            "name": "tags",
            "is_list": true,
            "selectors": [
                {
                    "type": "css",
                    "value": "div.quote"
                }
            ],
            "children": [
                {
                    "name": "quote_tags",
                    "is_list": true,
                    "selectors": [
                        {
                            "type": "css",
                            "value": "div.tags a.tag"
                        }
                    ],
                    "transformers": [
                        {
                            "name": "strip",
                            "args": []
                        }
                    ]
                }
            ]
        }
    ]
}
{
  "name": "image_scraper",
  "base_url": "http://books.toscrape.com",
  "fields": [
    {
      "name": "images",
      "selector": "div.image_container img",
      "is_list": true,
      "attribute": "src",
      "transformers": ["strip"]
    },
    {
      "name": "page_link",
      "selector": "ul.breadcrumb li:last-child",
      "transformers": ["strip"]
    }
  ]
}